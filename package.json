{
  "name": "tclogformat",
  "version": "2.0.0",
  "description": "stringify objects into searchable strings",
  "main": "index.js",
  "scripts": {
    "pretest": "jshint index.js",
    "test": "mocha -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tcort/tclogformat.git"
  },
  "keywords": [
    "log",
    "format",
    "stringify",
    "json",
    "object",
    "toString",
    "convert"
  ],
  "author": "Thomas Cort <linuxgeek@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tcort/tclogformat/issues"
  },
  "homepage": "https://github.com/tcort/tclogformat#readme",
  "dependencies": {
    "flat": "^4.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "jshint": "^2.9.6",
    "mocha": "^5.2.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "globalstrict": true,
    "immed": true,
    "indent": 4,
    "moz": true,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "trailing": true,
    "undef": true,
    "smarttabs": true,
    "strict": true,
    "validthis": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  }
